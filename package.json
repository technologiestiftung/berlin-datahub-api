    "helmet": "4.5.0",
    "http-errors": "1.8.0",
    "jsonwebtoken": "8.5.1",
    "logdna-winston": "3.0.5",
    "morgan": "1.10.0",
    "winston": "3.3.3"
  },
  "repository": {
    "type": "git",
