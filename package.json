    "@prisma/cli": "2.20.1",
    "@prisma/client": "2.27.0",
    "@types/bcrypt": "3.0.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "5.1.3",
    "@types/helmet": "4.0.0",
    "@types/http-errors": "1.8.1",
    "@types/jsonwebtoken": "8.5.4",
    "@types/morgan": "1.9.3",
    "@types/supertest": "2.0.11",
    "dotenv": "8.6.0",
    "jest-environment-node": "26.6.2",
    "nanoid": "3.1.23",
    "supertest": "6.1.3",
